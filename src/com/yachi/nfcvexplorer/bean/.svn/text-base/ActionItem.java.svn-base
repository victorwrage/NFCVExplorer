public class ActionItem {
	private long id;
	/** 卡ID */
	private String card_id = "0000000000";
	/** 动作类型 */
	private int action_type;
	/** 电话号码 */
	private String dia_number;
	/** 短信内容 */
	private String message_content;
	/** 程序包名 */
	private String package_name;
	/** 屏幕亮度值 */
	private int screen_light;
	/** 铃声类型 */
	private String ring_type;
	/** 铃声名字 */
	private String ring_name;
	/** 铃声音量 */
	private int ring_value;
	/** ACTION描述标题 */
	private String detail;
	/** ACTION描述值 */
	private String action_value;
	/** ACTION可用否 */
	private boolean action_enable = true;
	/** TAG创建时间*/
	private String create_time;
	/** 事件开关 */
	private boolean action_switch;
	/** ACTION打开应用程序 */
	public static final int ACTION_OPEN_APPLICATION = 0;
	/** ACTION关开WIFI */
	public static final int ACTION_WIFI = ACTION_OPEN_APPLICATION + 1;
	/** ACTION关开蓝牙 */
	public static final int ACTION_BLUETOOTH = ACTION_WIFI + 1;
	/** ACTION拨电话 */
	public static final int ACTION_DIA = ACTION_BLUETOOTH + 1;
	/** ACTION发短信 */
	public static final int ACTION_MESSAGE = ACTION_DIA + 1;
	/** ACTION调屏幕亮度 */
	public static final int ACTION_SCREEN_LIGHT = ACTION_MESSAGE + 1;
	/** ACTION锁屏 */
	public static final int ACTION_SCREEN_LOCK = ACTION_SCREEN_LIGHT + 1;
	/** ACTION铃声类型 */
	public static final int ACTION_RING_TYPE = ACTION_SCREEN_LOCK + 1;
	/** ACTION铃声音量 */
	public static final int ACTION_RING_VALUE = ACTION_RING_TYPE + 1;
	/** ACTION铃声名字 */
	public static final int ACTION_RING_NAME = ACTION_RING_VALUE + 1;
	
	/**
	 * @return the id
	 */
	public long getId() {
		return id;
	}

	/**
	 * @param id the id to set
	 */
	public void setId(long id) {
		this.id = id;
	}

	/**
	 * @return the create_time
	 */
	public String getCreate_time() {
		return create_time;
	}

	/**
	 * @param create_time the create_time to set
	 */
	public void setCreate_time(String create_time) {
		this.create_time = create_time;
	}

	/**
	 * @return the card_id
	 */
	public String getCard_id() {
		return card_id;
	}

	/**
	 * @param card_id
	 *            the card_id to set
	 */
	public void setCard_id(String card_id) {
		this.card_id = card_id;
	}

	/**
	 * @return the action_type
	 */
	public int getAction_type() {
		return action_type;
	}

	/**
	 * @param action_type
	 *            the action_type to set
	 */
	public void setAction_type(int action_type) {
		this.action_type = action_type;
	}

	/**
	 * @return the dia_number
	 */
	public String getDia_number() {
		return dia_number;
	}

	/**
	 * @param dia_number
	 *            the dia_number to set
	 */
	public void setDia_number(String dia_number) {
		this.dia_number = dia_number;
	}

	/**
	 * @return the message_content
	 */
	public String getMessage_content() {
		return message_content;
	}

	/**
	 * @param message_content
	 *            the message_content to set
	 */
	public void setMessage_content(String message_content) {
		this.message_content = message_content;
	}

	/**
	 * @return the package_name
	 */
	public String getPackage_name() {
		return package_name;
	}

	/**
	 * @param package_name
	 *            the package_name to set
	 */
	public void setPackage_name(String package_name) {
		this.package_name = package_name;
	}

	/**
	 * @return the screen_light
	 */
	public int getScreen_light() {
		return screen_light;
	}

	/**
	 * @param screen_light
	 *            the screen_light to set
	 */
	public void setScreen_light(int screen_light) {
		this.screen_light = screen_light;
	}

	/**
	 * @return the ring_name
	 */
	public String getRing_name() {
		return ring_name;
	}

	/**
	 * @param ring_name
	 *            the ring_name to set
	 */
	public void setRing_name(String ring_name) {
		this.ring_name = ring_name;
	}

	/**
	 * @return the ring_value
	 */
	public int getRing_value() {
		return ring_value;
	}

	/**
	 * @param ring_value
	 *            the ring_value to set
	 */
	public void setRing_value(int ring_value) {
		this.ring_value = ring_value;
	}

	/**
	 * @return the detail
	 */
	public String getDetail() {
		return detail;
	}

	/**
	 * @param detail
	 *            the detail to set
	 */
	public void setDetail(String detail) {
		this.detail = detail;
	}

	/**
	 * @return the action_switch
	 */
	public boolean getAction_switch() {
		return action_switch;
	}

	/**
	 * @param action_switch
	 *            the action_switch to set
	 */
	public void setAction_switch(boolean action_switch) {
		this.action_switch = action_switch;
	}

	/**
	 * @return the ring_type
	 */
	public String getRing_type() {
		return ring_type;
	}

	/**
	 * @param ring_type
	 *            the ring_type to set
